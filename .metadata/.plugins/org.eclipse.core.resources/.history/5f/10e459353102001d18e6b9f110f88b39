package metatrader.entities;

public class Moeda {
	
	private String nome;
	private Double precoVenda;
	private Double precoCompra;

	
	
	public Moeda(String nome, Double precoVenda, Double precoCompra) {
		this.nome = nome;
		this.precoVenda = precoVenda;
		this.precoCompra = precoCompra;
	}
	
	
}
